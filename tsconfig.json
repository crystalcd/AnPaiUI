{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ap-dw": [
        "dist/ap-dw"
      ],
      "ap-dw/*": [
        "dist/ap-dw/*"
      ],
      "ngx-tp": [
        "dist/ngx-tp"
      ],
      "ngx-tp/*": [
        "dist/ngx-tp/*"
      ]
    }
  }
}